<div class="container", id="sign_up_form">
      <div id="title_sign_up">
        See only what you care about!
      </div>
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %><%= devise_error_messages! %>
      <div class="row">
        <div class="col-xs-6 col-xs-offset-3 col-sm-6 col-sm-offset-2 col-md-4 col-md-offset-4">
          <div class="form-group">
            <%= f.label :name %><br>
            <%= f.text_field :name, class: "form-control", autofocus: true %>
          </div>
          <div class="form-group">
            <%= f.label :username %><br>
            <%= f.text_field :username, class: "form-control", autocomplete: "off"%>
          </div>
          <div class="form-group">
            <%= f.label :gender %><br>
            <%= f.radio_button :gender, true %> Male <br>
            <%= f.radio_button :gender, false %> Female <br>
          </div>
          <div class="form-group">
            <%= f.label :birthday %><br>
            <%= f.date_select :birthday, start_year: 1970, end_year: 2016, class: "form-control"%>
          </div>
          <div class="form-group">
            <%= f.label :email %><br>
            <%= f.email_field :email, class: "form-control", autocomplete: "off"%>
          </div>
          <div class="form-group">
            <%= f.label :password %><%if @minimum_password_length %><em>(<%= @minimum_password_length %> characters minimum)</em> <%end %><br>
            <%= f.password_field :password, class: "form-control", autocomplete: "off" %>
          </div>
          <div class="form-group">
            <%= f.label :password_confirmation %><br>
            <%= f.password_field :password_confirmation, class: "form-control", autocomplete: "off" %>
          </div>
          <div class="form-group btn_devise">
            <div>
              <%= f.submit "Sign up", class: "btn btn-primary form-control" %>
            </div>
          </div>
          <p class="text-center">or<p>
          <%end %>
          <%#= render "devise/shared/links" %>
        </div>
    </div>
</div>